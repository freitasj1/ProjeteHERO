void moveleft(){
  if(pos>=0){
    pos=pos-1;
    myservo.write(pos);
    delay(15);
  }
}
void moveright(){
  if(pos<=180){
    pos=pos+1;
    myservo.write(pos);
    delay(15);
  }
}

void eyeMoves(){
  blink_eye();
  lookright();lookright();
  blink_eye();
  lookleft();lookleft();
}

void lookaround(){
   //Servo Look around
  myservo.attach(10);
  for(pos = 90; pos <= 180; pos += 1){
        myservo.write(pos);
        delay(15);
      }
      for(; pos >= 0; pos-= 1) {
        myservo.write(pos);
        delay(15);
     }
     for(; pos<=90; pos += 1) {
        myservo.write(pos);
        delay(15);
     }
}

void setup() {
  //OLED
  Serial.begin(9600);
  if(!display.begin(SSD1306_SWITCHCAPVCC, 0x3C)) {
    Serial.println(F("SSD1306 allocation failed"));
  }
  display.clearDisplay();

  display_JF();
  eyeMoves();
  lookaround();
  
  pinMode(RIGHT, INPUT);
  pinMode(LEFT, INPUT);

}

void loop() {
  //Read values for movement
  int Right_Value = digitalRead(RIGHT);
  int Left_Value = digitalRead(LEFT);

  Serial.print("RIGHT");
  Serial.println(Right_Value);
  Serial.print("LEFT");
  Serial.println(Left_Value);

  if(Right_Value==1 && Left_Value==1){    
    eyeMoves();  
  }else if(Right_Value==0 && Left_Value==1) {    
    moveright();
  }else if(Right_Value==1 && Left_Value==0) {    
    moveleft();
  }else if(Right_Value==0 && Left_Value==0) {    
    blink_eye();
  }
}
